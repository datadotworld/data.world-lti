  },
  "main": "src/index.ts",
  "engines": {
    "node": "^9.6.1",
    "npm": "^5.6.0"
  },
  "scripts": {
    "build:development": "node_modules/.bin/webpack --config webpack.config.dev.js --watch",
    "build:production": "node_modules/.bin/webpack --env.NODE_ENV=production",
    "build:test": "node_modules/.bin/webpack --env.NODE_ENV=test",
    "postinstall": "npm run build:production && npm run migrate:up",
    "migrate:down": "node_modules/.bin/ts-node src/database/utilities/migrate.ts down",
    "migrate:next": "node_modules/.bin/ts-node src/database/utilities/migrate.ts next",
    "migrate:previous": "node_modules/.bin/ts-node src/database/utilities/migrate.ts previous",
    "migrate:status": "node_modules/.bin/ts-node src/database/utilities/migrate.ts status",
    "migrate:up": "node_modules/.bin/ts-node src/database/utilities/migrate.ts up",
    "start": "node_modules/.bin/ts-node src/index.ts",
    "start:development": "NODE_ENV=development npm rum start",
    "test": "mocha -r ts-node/register ./dist/tests.bundle.js"
  },
